<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="forum">
	<select id="forumDetail" parameterType="int" resultType="fvo">
		select
		*
		from
		forum
		where
		f_num = #{f_num}
	</select>
</mapper>